  <body>
    <section id="top">
      <div id="particles-js" style="display: none;"></div>
      <div id="particles-2-js"></div>
      <div class="container-fluid">
        <nav class="navbar navbar-expand-lg navbar-dark">
          <div class="container-fluid">
            <a class="brand-heading navbar-brand" href="#">
              <i class="fas fa-gamepad navbar-icon"></i> FA</a
            >
            <button
              class="navbar-toggler"
              type="button"
              data-bs-toggle="collapse"
              data-bs-target="#navbarSupportedContent"
              aria-controls="navbarSupportedContent"
              aria-expanded="false"
              aria-label="Toggle navigation"
            >
              <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
              <ul class="header-list navbar-nav ms-auto me-auto mb-2 mb-lg-0">
                <li class="nav-item nav-heading">
                  <a class="nav-link" href="https://theforcearcadium.com/">Home</a>
                </li>
                <li class="nav-item nav-heading">
                  <a class="nav-link" href="https://theforcearcadium.com/#about">About</a>
                </li>
                <li class="nav-item nav-heading">
                  <a class="nav-link" href="https://theforcearcadium.com/#divisions">Divisions</a>
                </li>
                <li class="nav-item nav-heading">
                  <a class="nav-link" href="https://theforcearcadium.com/#footer">Socials</a>
                </li>
                <li class="nav-item nav-heading">
                  <a class="nav-link" href="/release-blog">Blog</a>
                </li>
              </ul>
              <a href="https://discord.gg/nmNqQSwMqq" target="_blank">
                <button
                  type="button"
                  class="btn navbar-join-btn btn-outline-info"
                >
                  Join
                </button>
              </a>
            </div>
          </div>
        </nav>

        <div class="container-fluid heading-container">
          <h1>Meet Our Team</h1>
        </div>
        <div class="container-fluid admin">
          <h2>Administrators</h2>
          <div class="row">
            <% admins.forEach(function(admin){ %> 
            <div class="col-md-6" data-aos="zoom-in">
              <div class="staff-card admin-card">
                <img class="mb-3" src="<%= admin.avatar %>" alt="pfp">
                <hr>
                <h3><%= admin.name %> </h3>
                <em><h4>Admin</h4></em>
              </div>
            </div>
            <% }); %>    
            
          </div>

        </div>

        <div class="container-fluid developer">
          <h2>Web Developer</h2>
          <div class="row">
            <% developers.forEach(function(developer){ %> 
            <div class="col-md-6" data-aos="zoom-in" style="margin: auto;">
              <div class="staff-card developer-card">
                <img class="mb-3" src=<%= developer.avatar %>  alt="pfp">
                <hr>
                <h3><%= developer.name %></h3>
                <em><h4>Web Developer</h4></em>
              </div>
            </div>
            <% }); %> 
          </div>
        </div>

        <div class="container-fluid organizer">
          <h2>Organizers</h2>
          <div class="row">
            <% organizers.forEach(function(organizer){ %> 
            <div class="col-md-6" data-aos="zoom-in">
              <div class="staff-card organizer-card">
                <img class="mb-3" src=<%= organizer.avatar %> alt="pfp">
                <hr>
                <h3><%= organizer.name %> </h3>
                <em><h4>Organizer</h4></em>
              </div>
            </div>
            <% }); %> 
          
          </div>

        </div>
      </section>

    <!-- Footer -->
    <section id="footer">
      <div class="container-fluid">
        <div class="footer-icons-div">
          <a href="https://twitter.com/FArcadium/"
            ><i class="fab f-icons fa-3x fa-twitter twitter-icon"></i
          ></a>
          <a href=""
            ><i class="fab f-icons fa-3x fa-instagram instagram-icon"></i
          ></a>
          <a href="https://www.youtube.com/channel/UC500wIPRi_V9a2QD6t3LZBg"
            ><i class="fab f-icons fa-3x fa-youtube youtube-icon"></i
          ></a>
          <a href="https://open.spotify.com/show/0oYYNfsUdENb7N8anHZ00K"
            ><i class="fab f-icons fa-3x fa-spotify spotify-icon"></i
          ></a>
        </div>
        <div class="footer-p-div">
          <p>2021 Â© Force Arcadium</p>
        </div>
      </div>
    </section>

    <!-- jQuery CDN -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>

    <!-- Bootstrap JS -->

      <meta property="og:site_name" content="Force Arcadium" />

      <!-- Open Graph / Facebook -->
      <meta property="og:type" content="website" />
      <meta
        property="og:url"
        content="https://sahil-mhatre14.github.io/Force-Arcadium/"
      />
      <meta property="og:title" content="Force Arcadium" />
      <meta
        property="og:description"
        content="The Force Arcadium is a multi-game community currently serving gamers across various titles. We are players gathered together under a single banner to game and build lasting friendships. Join us in this exciting gaming adventure and meet amazing people!"
